            "program": "scripts/train_bev_topk2_model.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "args": [
                "configs/topk2/nuscenes_pointbev_evidential_topk2_vehicle_d1.yaml",
                "-g",
                "0", "1"
            ]
        },
        {
